{
  "name": "react-native-outside",
  "version": "0.1.18",
  "description": "React components for react-native click outside",
  "keywords": [
    "react",
    "native",
    "react-native",
    "web",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-contains",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:browser --config wtr.config.mjs",
    "test:engines": "tsds test:browser --config wtr.config.mjs",
    "version": "tsds version"
  },
  "devDependencies": {
    "@gorhom/portal": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/react": "^18.3.1",
    "@web/dev-server-import-maps": "*",
    "react": "^18.3.1",
    "react-native": "*",
    "react-native-contains": "*",
    "react-native-event": "*",
    "react-native-web": "*",
    "react-ref-boundary": "*",
    "react-test-renderer": "^18.3.1",
    "tsds-web-test-runner": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-contains": "*",
    "react-native-event": "*",
    "react-ref-boundary": "*"
  },
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
