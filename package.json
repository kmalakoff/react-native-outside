{
  "name": "react-native-outside",
  "version": "0.1.3",
  "description": "React components for react-native click outside",
  "keywords": [
    "react",
    "native",
    "react-native",
    "web",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-contains",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "npm run test:rn",
    "test:engines": "nvu engines npm test",
    "test:rn": "tsds link && ts-swc mocha --watch-extensions ts,tsx test/**/*.test-rn.* --loader=./test/lib/alias-rnw.mjs && tsds unlink",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@gorhom/portal": "^1.0.14",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.79",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-native-contains": "^0.1.11",
    "react-native-event": "^0.2.4",
    "react-native-web": "^0.19.11",
    "react-ref-boundary": "^0.1.4",
    "react-test-renderer": "^18.2.0",
    "ts-dev-stack": "^0.15.8"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-contains": "*",
    "react-native-event": "*",
    "react-ref-boundary": "*"
  },
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
