{
  "name": "react-native-outside",
  "version": "0.1.1",
  "description": "React components for react-native click outside",
  "keywords": [
    "react",
    "native",
    "react-native",
    "web",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-contains",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/react-native-contains.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "coverage": "tsds link && cross-env NODE_OPTIONS=--experimental-vm-modules jest test/**/*.test.* --collect-coverage",
    "deploy": "tsds deploy",
    "format": "tsds format",
    "lint": "tsds lint",
    "test": "tsds link && cross-env NODE_OPTIONS=--experimental-vm-modules jest jest test/**/*.test.*",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@gorhom/portal": "^1.0.14",
    "@types/jest": "^28.1.7",
    "@types/react": "18.0.15",
    "@types/react-native": "^0.69.1",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.76.0",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-contains": "^0.1.10",
    "react-native-event": "^0.2.1",
    "react-ref-boundary": "^0.1.2",
    "react-test-renderer": "18.0.0",
    "ts-dev-stack": "^0.8.10"
  },
  "peerDependencies": {
    "react": "*"
  },
  "optionalDependencies": {
    "react-native": "*",
    "react-native-contains": "*",
    "react-native-event": "*",
    "react-ref-boundary": "*"
  },
  "engines": {
    "node": ">=14"
  },
  "tsds": {
    "source": "src/index.ts",
    "globals": {
      "react": "React",
      "react-native": "ReactNative",
      "react-native-contains": "reactNativeContains",
      "react-native-event": "reactNativeEvent",
      "react-ref-boundary": "reactRefBoundary"
    }
  }
}
