{
  "name": "react-native-outside",
  "version": "0.1.3",
  "description": "React components for react-native click outside",
  "keywords": [
    "react",
    "native",
    "react-native",
    "web",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-contains",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "npm run test:rn",
    "test:engines": "nvu engines npm test",
    "test:rn": "tsds link && ts-swc mocha --watch-extensions ts,tsx test/**/*.test-rn.* --loader=./test/lib/alias-rnw.mjs && tsds unlink",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@gorhom/portal": "^1.0.14",
    "@types/mocha": "^10.0.10",
    "@types/react": "^18.3.1",
    "react": "^18.3.1",
    "react-native": "^0.76.5",
    "react-native-contains": "^0.1.11",
    "react-native-event": "^0.2.4",
    "react-native-web": "^0.19.13",
    "react-ref-boundary": "^0.1.4",
    "react-test-renderer": "^18.3.1",
    "ts-swc-loaders": "^1.5.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-contains": "*",
    "react-native-event": "*",
    "react-ref-boundary": "*"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
